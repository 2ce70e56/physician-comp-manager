datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Organization {
  id            String     @id @default(cuid())
  name          String
  providers     Provider[]
  contracts     Contract[]
  createdAt     DateTime   @default(now())
  updatedAt     DateTime   @updatedAt
}